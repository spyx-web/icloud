{
    "name": "szwtdl/icloud",
    "description": "apple icloud sdk",
    "type": "library",
    "keywords": ["icloud", "laravel-icloud","icloud-sdk"],
    "authors": [
        {
            "name": "szwtdl",
            "email": "szpengjian@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^7.4",
        "nesbot/carbon": "^2.58",
        "symfony/dom-crawler": "^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "mockery/mockery": "^1.5",
        "phpstan/phpstan": "^1.8",
        "jetbrains/phpstorm-attributes": "^1.0",
        "friendsofphp/php-cs-fixer": "^3.11"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Cloud\\": "src/"
        },
        "files": [
            "src/Helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Cloud\\ServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable"
}
