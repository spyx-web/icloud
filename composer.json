{
    "name": "szwtdl/icloud",
    "description": "apple icloud sdk",
    "type": "library",
    "require": {
        "ext-json": "*",
        "guzzlehttp/guzzle": "^7.4",
        "friendsofphp/php-cs-fixer": "^3.9"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "mockery/mockery": "^1.5"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Cloud\\": "src/"
        },
        "files": [
            "src/Helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "szwtdl",
            "email": "szpengjian@gmail.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Cloud\\ServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable"
}
